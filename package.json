  "scripts": {
    "start:dev": "NODE_ENV=development node --require dotenv/config server/start.js",
    "start": "NODE_ENV=staging node --require dotenv/config server/start.js",
    "startOLD": "NODE_ENV=production node --require server/start.js",
    "build": "node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/client/unit/karma.conf.js --single-run",
    "e2e": "node test/client/e2e/runner.js",
    "test:client": "npm run unit && npm run e2e",
    "test:server": "lab -a code -t 100 -v",
    "test2": "npm run test:client && npm run test:server",
    "lint": "eslint --ext .js,.vue src test/client/unit/specs test/client/e2e/specs",
    "knex-migrate": "node --require dotenv/config node_modules/knex/bin/cli.js migrate:latest",
    "knex-seed": "node --require dotenv/config node_modules/knex/bin/cli.js seed:run"
  },
